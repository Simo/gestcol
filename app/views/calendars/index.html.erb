<% title "Calendars" %>

<table>
  <tr>
    <th>Almanac</th>
    <th>Prospect Structure</th>
  </tr>
  <% for calendar in @calendars %>
    <tr>
      <td><%= calendar.almanac_id %></td>
      <td><%= calendar.prospect_structure_id %></td>
      <td><%= link_to "Show", calendar %></td>
      <td><%= link_to "Edit", edit_calendar_path(calendar) %></td>
      <td><%= link_to "Destroy", calendar, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Calendar", new_calendar_path %></p>
